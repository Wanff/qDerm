  <div class="container" style="text-align: center; color: white; padding-bottom: 50px;">
    <h1>Results</h1>
    <% var exists = true; %>
    <% try { %>
    <% if (data) %>
    <%      exists = true; %>
    <% } catch(e) { exists = false; } %>
    <% if (exists) { %>
      <% if(data == "Image does not possess the right dimensions :/"){ %>
        <div class="circle_cropper mt-3" style="color:red;">
          <p style="font-family: Proxima Nova;
          font-weight: 500;
          font-size: 20px;
          margin-top: 20px;
          margin-bottom: 0;"><%= data %></p>
        </div>
      <% } %>
      <% if (Number(data)==1){ %>
        <div class="circle_cropper mt-3" style="color:red;">
          <p style="font-family: Proxima Nova;
          font-weight: 500;
          font-size: 20px;
          margin-top: 20px;
          margin-bottom: 0;">Malignant</p>  
          <p class = "blood_pressure">Malignant</p>

        </div>
      <% } else if (Number(data)==0){ %>
        <div class="circle_cropper mt-3" style="color:green;">
          <p class = "blood_pressure">Benign</p>
        </div>
      <% } else { %>
        <div class="circle_cropper mt-3" style="color:white;">
          <p class = "blood_pressure"> Unknown </p>
      </div>
      <% } %>
      <% } %>
  </div>


